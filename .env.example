# Environment Configuration
EXPO_PUBLIC_ENV=development
APP_VARIANT=development

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:3001
EXPO_PUBLIC_API_TIMEOUT=10000

# Feature Flags
EXPO_PUBLIC_ENABLE_LOGGING=true
EXPO_PUBLIC_ENABLE_DEBUG=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# Database
EXPO_PUBLIC_DB_NAME=pet_tracker_dev_db

# External Services
EXPO_PUBLIC_SENTRY_DSN=

# Logging - BetterStack Configuration
EXPO_PUBLIC_BETTERSTACK_TOKEN=
EXPO_PUBLIC_BETTERSTACK_ENDPOINT=https://in.logs.betterstack.com

# Analytics - Choose ONE of these services:
# Firebase Analytics (recommended) - No key needed, uses google-services.json  
EXPO_PUBLIC_FIREBASE_ENABLED=false

# OR Mixpanel - Get from https://mixpanel.com/settings/project
EXPO_PUBLIC_MIXPANEL_TOKEN=

# OR Amplitude - Get from https://amplitude.com/settings
EXPO_PUBLIC_AMPLITUDE_API_KEY=

# Legacy key for backwards compatibility
EXPO_PUBLIC_ANALYTICS_KEY=

# App Store / Play Store
EXPO_PROJECT_ID=
